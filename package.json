{
  "name": "channel",
  "version": "1.0.0",
  "description": "A simple web chat application.",
  "main": "src/server",
  "scripts": {
    "start": "node src/server",
    "build:web-client": "npm run build --prefix web-client",
    "start:dev-server": "nodemon src/server",
    "start:dev-web-client": "npm run serve --prefix web-client",
    "dev": "concurrently \"npm run start:dev-server\" \"npm run start:dev-web-client\""
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.2.1",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bastilavarias/channel.git"
  },
  "author": "Sebastian Curtis T. Lavarias",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bastilavarias/channel/issues"
  },
  "homepage": "https://github.com/bastilavarias/channel#readme"
}
